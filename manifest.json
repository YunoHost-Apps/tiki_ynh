{
    "name": "Tiki Wiki CMS Groupware",
    "id": "tiki",
    "packaging_format": 1,
    "description": {
        "en": "The Free / Libre / Open Source Web Application with the most built-in features",
        "fr": "L'application Web libre dotée du plus grand nombre de fonctionnalités intégrées"
    },
    "version": "24.0~ynh1",
    "url": "https://tiki.org/",
    "upstream": {
        "license": "LGPL-2.1-only",
        "website": "https://tiki.org/",
        "demo": "https://tiki.org/Try-Tiki",
        "admindoc": "https://doc.tiki.org",
        "code": "https://gitlab.com/tikiwiki/tiki/"
    },
    "license": "GPL-2.0-only",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/tiki",
                "default": "/tiki"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
