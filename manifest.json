{
    "name": "Tiki",
    "id": "tiki",
    "packaging_format": 1,
    "description": {
        "en": "Wiki-based, content management system",
        "fr": "Système de gestion de contenu basé sur Wiki"
    },
    "version": "24.2~ynh1",
    "url": "https://tiki.org/",
    "upstream": {
        "license": "LGPL-2.1-only",
        "website": "https://tiki.org/",
        "demo": "https://tiki.org/Try-Tiki",
        "admindoc": "https://doc.tiki.org",
        "code": "https://gitlab.com/tikiwiki/tiki/"
    },
    "license": "LGPL-2.1-only",
    "maintainer": {
        "name": "eric_G",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/tiki",
                "default": "/tiki"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
