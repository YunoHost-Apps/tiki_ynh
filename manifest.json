{
    "name": "Tiki",
    "id": "tiki",
    "packaging_format": 1,
    "description": {
        "en": "Explain i what it actually does (it is meant to give a rough idea to users browsing a catalog of 100+ apps)",
        "fr": "Moteur de wiki"
    },
    "version": "1.0~ynh1",
    "url": "https://tiki.org/",
    "upstream": {
        "license": "free",
        "website": "https://tiki.org/",
        "demo": "https://tiki.org/Try-Tiki",
        "admindoc": "https://doc.tiki.org",
        "code": "https://gitlab.com/tikiwiki/tiki/"
    },
    "license": "free",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/tiki",
                "default": "/tiki"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
